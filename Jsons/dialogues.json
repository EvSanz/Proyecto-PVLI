      "scenes": [
        {
          "id": "Scene0",
          "opciones": 1,
          "irritacion": 0,
          "clock": false,
          "lines": [
            "Buenos días, amigo",
            "¿Por qué ha decidido viajar a la gran ciudad?",
            "¿Conoce usted a las hermanas Morton?"
          ]
        },

        {
          "id": "Scene1",
          "opciones": 7,
          "irritacion": 0,
          "clock": false,
          "lines": [
            "Digamos que mis compañeros de trabajo eran nefastos",
            "No entendían que el futuro de la arquitectura...",
            "son los edificios rectos, con todas sus piezas simetricas...",
            "¡Eficientes y hermosas!",
            "Y eso no es todo, porque-",
            "Perdone, me he dejado llevar",
            "No sería la primera persona que me comenta que hablo demasiado",
            "¿Alguna de esas personas eran las hermanas Morton?"
          ]
        },

        {
          "id": "Scene2",
          "opciones": 3,
          "irritacion": 25,
          "clock": false,
          "lines": [
            "¿Las hermanas pintoras?",
            "¿Por qué lo pregunta?",
            "Solo escuché que estaban pintando retratos a poco precio",
            "¿Y puede decirme que hizo ayer por la noche?",
            "Gracias por su ayuda"
          ]
        },

        {
          "id": "Scene3",
          "opciones": -1,
          "irritacion": 25,
          "clock": true,
          "lines": [
            "Dormir, como todo el mundo",
            "Bueno, todos menos los borrachos esos",
            "No sé quienes son, pero Alfred quizás sepa algo",
            "Ahora si me disculpas, estoy un poco ocupado"
          ]
        },

        {
          "id": "Scene4",
          "opciones": -1,
          "irritacion": 0,
          "clock": true,
          "lines": [
            "No hay de qué"
          ]
        }
      ]
    },

