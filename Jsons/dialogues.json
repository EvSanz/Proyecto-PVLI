      "scenes": [
        {
          "id": "Scene0",
          "opciones": 1,
          "irritacion": 0,
          "clock": false,
          "lines": [
            "¿En qué puedo ayudarlo, detective?",
            "¿Por qué ha decidido embarcarse en este tren?",
            "¿Qué estuvo haciendo ayer por la noche?"
          ]
        },

        {
          "id": "Scene1",
          "opciones": 1,
          "irritacion": 0,
          "clock": false,
          "lines": [
            "Voy a ver a unos inversores para exponer unas ideas",
            "Asumo que no salió de su cuarto entonces...",
            "¿Qué ideas?"
          ]
        },

        {
          "id": "Scene2",
          "opciones": 2,
          "irritacion": 10,
          "clock": false,
          "lines": [
            "Estuve trabajando en un pequeño proyecto",
            "Nada que sea relevante para usted",
            "¿En qué consiste el proyecto?",
            "Entiendo. Muchas gracias por su ayuda"
          ]
        },

        {
          "id": "Scene3",
          "opciones": 5,
          "irritacion": 0,
          "clock": true,
          "lines": [
            "¿De verdad está interesado?",
