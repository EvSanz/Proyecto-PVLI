        {
          "id": "Scene2",
          "opciones": 2,
          "irritacion": 0,
          "clock": false,
          "lines": [
            "¿Morton?",
            "No me suena",
            "¿Que hacía usted ayer a medianoche?",
            "Entiendo. Muchas gracias por tu ayuda"
          ]
        },

        {
          "id": "Scene3",
          "opciones": -1,
          "irritacion": 25,
          "clock": true,
          "lines": [
            "¿A usted qué le importa?",
            "¿Se cree que también trabajo de noche?",
            "Hágame un favor y dejeme en paz, picoleto"
          ]
        },

        {
          "id": "Scene4",
          "opciones": -1,
          "irritacion": 0,
          "clock": true,
          "lines": [
            "Sí, sí, lo que sea"
          ]
        }
      ]
    },

